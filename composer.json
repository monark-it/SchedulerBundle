{
    "name": "multiinfo/scheduler-bundle",
    "description": "MIT Scheduler Bundle",
    "type": "Symfony Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "MIT",
            "email": "contact@mit-agency.com"
        }
